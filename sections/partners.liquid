<style>
  .partners{
    background-color: {{section.settings.background-color}}
  }
</style>
<div class="container partners">
  {{ section.settings['partner-logo'] | image_url: width: 140 | image_tag }}
</div>
{% schema %}
{
  "name": "Partners",
  "settings": [
    {
      "type": "color",
      "id": "background-color",
      "label": "Background Color",
      "default":"#666666"
    },
    {
      "type": "image_picker",
      "id": "partner-logo",
      "label": "Partner Logo"
    }
  ]
}
{% endschema %}
